                        //Please enter your package tracking number!
                        ShowMessageBox("Invalid Entry", "Please enter your tracking number!!!");

                        //$("#btnTrack").html("Track");
                        //$("#btnTrack").prop("disabled", false);
                        $btn.button('reset');
                    }
                    else
                    {
                        $.get('@Url.Action("CheckTrackingNumber", "Mathely_Logistics_Shipments")', { TrackingNumber: $.trim($("#TrackingNumber").val()) }, function (data)
                        {
                            if (data === "Not Found")
