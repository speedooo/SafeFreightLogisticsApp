<div class="hero-3 full-section " id="banner">
    <div class="container">
        <div class="row">
            <div class="col-md-7">
                <h2>Track Your Shipment</h2>
                <div class="form">
                    @using ( Html.BeginForm() )
                    {
                        @Html.TextBox( "TrackingNumber", null, new {@class = "form-control", @placeholder = "Your Tracking Number eg. (12345-12345)"} )
                        <button id="btnTrack" type="submit" data-loading-text = "Wait...">Track</button>
                    }
                    @*<span class="declaration">We hate spam. You can trust us, we won't use your email</span>*@
                </div>
            </div>
            <div class="col-md-5">
                <p class="lead text-justify">SafeFreight Logistics is an international freight forwarder for international businesses. Innovative operational techniques enable us to enjoy almost immediate success and growth. We achieve this by assisting them to align the right strategies for their input supply, product distribution and costs reduction.</p>
            </div>
        </div>
    </div>
</div>
<!--======= HOME Banner END =========-->




@section scripts
{
    <!-- Jquery For Home Page Only  -->
<!-- Revolution Slider 5.x SCRIPTS -->
<script src="~/content/js/revolution-slider/js/jquery.themepunch.tools.min.js"></script>
<script src="~/content/js/revolution-slider/js/jquery.themepunch.revolution.min.js"></script>
<script src="~/content/js/custom.js"></script>
<script type="text/javascript">
              $(document).ready(function(e) {
                var revapi = $(".rev_slider").revolution({
                  sliderType:"standard",
                  jsFileLocation: "~/content/js/revolution-slider/js/",
                  sliderLayout: "auto",
                  dottedOverlay: "none",
                  delay: 5000,
                  navigation: {
                      keyboardNavigation: "off",
                      keyboard_direction: "horizontal",
                      mouseScrollNavigation: "off",
                      onHoverStop: "off",
                      touch: {
                          touchenabled: "on",
                          swipe_threshold: 75,
                          swipe_min_touches: 1,
                          swipe_direction: "horizontal",
                          drag_block_vertical: false
                      },
                      arrows: {
                          style: "gyges",
                          enable: true,
                          hide_onmobile: false,
                          hide_onleave: true,
                          hide_delay: 200,
                          hide_delay_mobile: 1200,
                          tmp: '',
                          left: {
                              h_align: "left",
                              v_align: "center",
                              h_offset: 0,
                              v_offset: 0
                          },
                          right: {
                              h_align: "right",
                              v_align: "center",
                              h_offset: 0,
                              v_offset: 0
                          }
                      },

                        bullets: {
                        enable: true,
                        hide_onmobile: true,
                        hide_under: 800,
                        style: "hebe",
                        hide_onleave: false,
                        direction: "horizontal",
                        h_align: "center",
                        v_align: "bottom",
                        h_offset: 0,
